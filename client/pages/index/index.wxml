<view class="page">
    <view class="weui-panel" bindtap='noteInfo'>
      <view class="weui-panel__hd detHd">
          <view class='detTitle'>随手记</view>
          <view class='detSysTime'>{{newNote.sysTime}}{{newNote.weekDay}}</view>
      </view>
      <view class="weui-panel__bd">
          <view class="weui-media-box weui-media-box_text">
              <view class="weui-media-box__desc detContent">{{newNote.title}}</view> 
          </view>
      </view>
    </view>
    <view class="weui-panel" bindtap='shopInfo'>
      <view class="weui-panel__hd detHd">
          <view class='detTitle'>消费单：{{weekShop}}￥(本周)</view>
          <view class='detSysTime'>{{newShop.sysTime}}{{newShop.weekDay}}</view>
      </view>
      <!-- <view class="weui-panel__bd">
          <view class="weui-media-box weui-media-box_text">
              <view class="weui-media-box__desc detContent">{{newShop.shopComment}}</view> 
          </view>
      </view> -->
    </view>
</view>